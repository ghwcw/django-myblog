<?xml version="1.0" encoding="UTF-8"?>
    <configuration>
        <system.webServer>
            <handlers>
                <add name="Python FastCGI" path="*" verb="*" modules="FastCgiModule" scriptProcessor="d:\djblog\vpy-myblog\scripts\python.exe|d:\djblog\vpy-myblog\lib\site-packages\wfastcgi.py" resourceType="Unspecified" requireAccess="Script" />
            </handlers>
        <directoryBrowse enabled="true" />
        </system.webServer>
        <appSettings>
            <add key="WSGI_HANDLER" value="django.core.wsgi.get_wsgi_application()" />
            <add key="PYTHONPATH" value="D:\DjBlog\myblog" />
            <add key="DJANGO_SETTINGS_MODULE" value="myblog.settings" />
        </appSettings>
    </configuration>